{
  "functions": {
    "app/api/docente/reportes/route.ts": {
      "maxDuration": 300,
      "memory": 2048,
      "runtime": "nodejs18.x"
    }
  },
  "build": {
    "env": {
      "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "false",
      "PLAYWRIGHT_BROWSERS_PATH": "0",
      "NODE_OPTIONS": "--max-http-header-size=16384"
    }
  },
  "headers": [
    {
      "source": "/api/docente/reportes/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/reports/:path*",
      "destination": "/api/reports/:path*"
    }
  ]
}
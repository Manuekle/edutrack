{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@4.0.0"
    },
    {
      "src": "app/api/docente/reportes/route.ts",
      "use": "@vercel/node@4.0.0",
      "config": {
        "maxDuration": 300,
        "memory": 2048,
        "runtime": "nodejs18.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "false",
    "PLAYWRIGHT_BROWSERS_PATH": "0",
    "NODE_OPTIONS": "--max-http-header-size=16384"
  }
}
{
  "name": "edutrack",
  "version": "1.0.0",
  "type": "module",
  "description": "Software para la gestión automatizada de asistencias estudiantiles mediante códigos QR",
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "check": "pnpm run format:check && pnpm run type-check",
    "build": "prisma generate && next build",
    "seed": "prisma db seed",
    "start": "next start",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:api": "jest --config jest.api.config.cjs --passWithNoTests tests/api",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "jest --passWithNoTests && playwright test",
    "validate": "pnpm run type-check && pnpm run test",
    "prepare": "command -v husky >/dev/null 2>&1 && husky || true",
    "postbuild": "next-sitemap",
    "postinstall": "prisma generate",
    "update": "ncu -u"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manuekle/edutrack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Manuekle/edutrack/issues"
  },
  "homepage": "https://github.com/Manuekle/edutrack#readme",
  "resolutions": {
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1"
  },
  "overrides": {
    "next-auth": {
      "nodemailer": "$nodemailer"
    }
  },
